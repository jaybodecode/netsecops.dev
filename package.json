{
  "name": "new-site",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "PORT=3000 nuxt dev",
    "pregenerate": "[ -d .output/public/.git ] && cp -r .output/public/.git /tmp/netsecops-git-backup || true",
    "generate": "nuxt generate && cp -r github-pages-config/* github-pages-config/.nojekyll .output/public/",
    "postgenerate": "[ -d /tmp/netsecops-git-backup ] && cp -r /tmp/netsecops-git-backup .output/public/.git && rm -rf /tmp/netsecops-git-backup || true",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "nuxi typecheck"
  },
  "dependencies": {
    "@genkit-ai/google-genai": "^1.21.0",
    "@google-cloud/vertexai": "^1.10.0",
    "@google/generative-ai": "^0.24.1",
    "@nuxt/eslint": "^1.9.0",
    "@nuxt/fonts": "^0.11.4",
    "@nuxt/icon": "^2.0.0",
    "@nuxt/image": "^1.11.0",
    "@types/better-sqlite3": "^7.6.13",
    "better-sqlite3": "^12.4.1",
    "cheerio": "^1.1.2",
    "commander": "^14.0.1",
    "dotenv": "^17.2.3",
    "eslint": "^9.37.0",
    "genkit": "^1.21.0",
    "marked": "^16.4.0",
    "nuxt": "^4.1.3",
    "puppeteer": "^24.25.0",
    "stopword": "^3.1.5",
    "twitter-api-v2": "^1.27.0",
    "vue": "^3.5.22",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@iconify-json/heroicons": "^1.2.3",
    "@iconify-json/mdi": "^1.2.3",
    "@nuxtjs/color-mode": "^3.5.2",
    "@nuxtjs/seo": "^3.2.2",
    "@nuxtjs/tailwindcss": "^6.14.0",
    "@tailwindcss/typography": "^0.5.19",
    "@types/stopword": "^2.0.3",
    "@vueuse/nuxt": "^13.9.0",
    "canvas": "^3.2.0",
    "eslint-formatter-compact": "^8.40.0",
    "nodemon": "^3.1.10",
    "nuxt-gtag": "^4.0.0",
    "playwright": "^1.56.1",
    "tailwindcss": "^3.4.18",
    "tsx": "^4.20.6"
  }
